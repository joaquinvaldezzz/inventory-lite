{
  "name": "inventory-lite",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:android": "ionic capacitor run android --livereload",
    "dev:ios": "ionic capacitor run ios --livereload",
    "build": "vite build",
    "build:android": "ionic capacitor build android --no-open",
    "build:ios": "ionic capacitor build ios --no-open",
    "preview": "vite preview",
    "serve": "vite build && vite preview",
    "format": "prettier --write .",
    "format:src": "prettier --write src",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "generate:icons": "capacitor-assets generate",
    "test.e2e": "cypress run",
    "test.unit": "vitest"
  },
  "dependencies": {
    "@capacitor/android": "^7.2.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/core": "^7.2.0",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.2.0",
    "@capacitor/keyboard": "^7.0.1",
    "@hookform/resolvers": "^5.0.1",
    "@ionic/react": "^8.5.7",
    "@ionic/react-router": "^8.5.7",
    "@ionic/storage": "^4.0.0",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.10",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-hover-card": "^1.1.13",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-menubar": "^1.1.14",
    "@radix-ui/react-navigation-menu": "^1.2.12",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
