{
  "name": "inventory-lite",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:android": "ionic capacitor run android --livereload",
    "dev:ios": "ionic capacitor run ios --livereload",
    "build": "vite build",
    "build:android": "ionic capacitor build android --no-open",
    "build:ios": "ionic capacitor build ios --no-open",
    "preview": "vite preview",
    "serve": "vite build && vite preview",
    "format": "prettier --write .",
    "format:src": "prettier --write src",
    "lint": "eslint --cache",
    "lint:fix": "eslint --fix",
    "generate:icons": "capacitor-assets generate",
    "test.e2e": "cypress run",
    "test.unit": "vitest"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.0",
    "@capacitor/app": "6.0.2",
    "@capacitor/core": "6.2.0",
    "@capacitor/haptics": "6.0.2",
    "@capacitor/ios": "^6.2.1",
    "@capacitor/keyboard": "6.0.3",
    "@hookform/resolvers": "^4.1.3",
    "@ionic/react": "^8.5.2",
    "@ionic/react-router": "^8.5.2",
    "@ionic/storage": "^4.0.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@t3-oss/env-core": "^0.12.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-table": "^8.21.2",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.5.2",
    "input-otp": "^1.4.2",
    "ionicons": "^7.4.0",
    "jose": "^6.0.8",
    "lucide-react": "^0.485.0",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-aria-components": "^1.6.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-resizable-panels": "^2.1.7",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "recharts": "^2.15.1",
    "sonner": "^2.0.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-safe-area": "^0.6.0",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "7.1.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@tailwindcss/vite": "^4.0.17",
    "@tanstack/eslint-plugin-query": "^5.66.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "cypress": "^14.2.1",
    "eslint": "^9.19.0",
    "eslint-config-love": "^119.0.0",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-perfectionist": "^4.7.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-packagejson": "^2.5.8",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.0.17",
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.11",
    "vitest": "^3.0.7"
  },
  "prettier": {
    "printWidth": 100,
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports",
      "prettier-plugin-jsdoc",
      "prettier-plugin-tailwindcss"
    ],
    "importOrder": [
      "^(react/(.*)$)|^(react$)",
      "^(@ionic/react/(.*)$)|^(@ionic/react$)",
      "^(@ionic/react-router/(.*)$)|^(@ionic/react-router$)",
      "<THIRD_PARTY_MODULES>",
      "",
      "^types$",
      "^@/types/(.*)$",
      "^@/config/(.*)$",
      "^@/lib/(.*)$",
      "^@/hooks/(.*)$",
      "^@/components/ui/(.*)$",
      "^@/components/(.*)$",
      "^@/registry/(.*)$",
      "^@/styles/(.*)$",
      "^@/app/(.*)$",
      "^@/pages/(.*)$",
      "",
      "^[./]"
    ],
    "tailwindStylesheet": "src/styles/main.css",
    "tailwindAttributes": [
      "classNames"
    ],
    "tailwindFunctions": [
      "cva",
      "cn"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "cypress"
    ]
  }
}
